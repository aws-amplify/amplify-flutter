{
  "name": "infra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "setup": "cdk bootstrap --profile=${AWS_PROFILE:=default}",
    "deploy": "cdk deploy --profile=${AWS_PROFILE:=default} --all --require-approval=never && dart pub get && ./tool/create_configs.dart",
    "destroy": "cdk destroy --profile=${AWS_PROFILE:=default}"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "aws-cdk": "2.1106.0",
    "esbuild": "^0.27.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^4.2.1",
    "@aws-sdk/client-amplify": "^3.993.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.993.0",
    "@aws-sdk/client-s3": "^3.993.0",
    "aws-cdk-lib": "2.238.0",
    "constructs": "^10.5.0",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21"
  },
  "overrides": {
    "fast-xml-parser": "^4.4.1",
    "js-yaml": "^4.4.1"
  }
}
