{
  "name": "infra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "setup": "cdk bootstrap --profile=${AWS_PROFILE:=default}",
    "deploy": "cdk deploy --profile=${AWS_PROFILE:=default} --all --require-approval=never && dart pub get && ./tool/create_configs.dart",
    "destroy": "cdk destroy --profile=${AWS_PROFILE:=default}"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.4",
    "aws-cdk": "2.1010.0",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^4.2.1",
    "@aws-sdk/client-amplify": "^3.840.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.840.0",
    "@aws-sdk/client-s3": "^3.840.0",
    "aws-cdk-lib": "2.190.0",
    "constructs": "^10.4.2",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21"
  },
  "overrides": {
    "fast-xml-parser": "^4.4.1"
  }
}
