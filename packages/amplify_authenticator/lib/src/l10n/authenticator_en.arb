    "@screenSignup": {
        "description": "Title of the Signup screen and form"
    },
    "screenConfirmSignup": "Enter your confirmation code",
    "@screenConfirmSignup": {
        "description": "Title of the Confirm Signup screen and form"
    },
    "screenConfirmSigninMfa": "Enter your sign in code",
    "@screenConfirmSigninMfa": {
        "description": "Title of the Confirm Signin with MFA screen and form"
    },
    "screenConfirmSigninNewPassword": "Change your password to sign in",
    "@screenConfirmSigninNewPassword": {
        "description": "Title of the Confirm Signin with New Password screen and form"
    },
    "screenResetPassword": "Reset your password",
    "@screenResetPassword": {
        "description": "Title of the Reset Password screen and form"
    },
    "screenSendCode": "Send Code",
    "@screenSendCode": {
        "description": "Title of the Send Code screen and form"
