            "previousScreen": {
                "type": "String",
                "example": "Sign In",
                "description": "The title of the previous screen."
            }
        }
    },
    "signInWith": "Sign In with {provider}",
    "@signInWith": {
        "description": "Label of button to sign in with a social provider",
        "placeholders": {
            "provider": {
                "type": "String",
                "example": "Google",
                "description": "The social sign-in provider"
            }
        }
    },
    "username": "Username",
    "@username": {
        "description": "User's chosen username."
    },
    "password": "Password",
    "@password": {
        "description": "User's chosen password."
    },
    "passwordConfirmation": "Confirm Password",
    "@passwordConfirmation": {
        "description": "Confirmation of user's chosen password."
    },
    "email": "Email",
    "@email": {
        "description": "User's preferred e-mail address.",
        "example": "john.doe@example.com"
    },
    "phoneNumber": "Phone Number",
    "@phoneNumber": {
        "description": "User's preferred telephone number.",
        "example": "+1 (888) 555-1234"
    },
    "verificationCode": "Verification Code",
    "@verificationCode": {
        "description": "The code sent to the user's phone number or email address for verification.",
        "example": "123456"
    },
    "address": "Address",
    "@address": {
        "description": "User's preferred postal address."
    },
    "birthdate": "Birthdate",
    "@birthdate": {
        "description": "User's birthday, represented as an ISO 8601:2004 [ISO8601â€‘2004] YYYY-MM-DD format.",
